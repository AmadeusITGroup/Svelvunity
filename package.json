{
	"name": "@amadeus-it-group/svelvunity",
	"description": "Reusable components and utilities svelte library.",
	"version": "0.0.0",
	"bugs": "https://github.com/AmadeusITGroup/Svelvunity/issues",
	"license": "MIT",
	"type": "module",
	"keywords": [
		"accordion",
		"datepicker",
		"components",
		"pagination",
		"popover",
		"reusable",
		"slider",
		"svelte",
		"tabs",
		"tailwindcss",
		"toast",
		"tooltip"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/AmadeusITGroup/Svelvunity.git"
	},
	"engines": {
		"npm": "^11.6.2",
		"node": "^24.12.0"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"dependencies": {
		"@eslint/compat": "^1.2.9",
		"@floating-ui/dom": "^1.7.4",
		"@tailwindcss/postcss": "^4.1.8",
		"htm": "^3.1.1",
		"svelte-preprocess": "^6.0.3",
		"tailwind-merge": "^3.3.1",
		"typescript-eslint": "^8.32.1"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^6.0.1",
		"@sveltejs/kit": "^2.50.0",
		"@sveltejs/package": "2.5.7",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/vite": "^4.1.7",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/svelte": "^5.2.8",
		"@testing-library/user-event": "14.6.1",
		"@typescript-eslint/eslint-plugin": "^8.46.2",
		"@typescript-eslint/parser": "^8.46.2",
		"@vitest/coverage-v8": "^4.0.10",
		"@vitest/ui": "^4.0.10",
		"autoprefixer": "^10.4.21",
		"eslint": "^9.38.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.11.0",
		"jsdom": "^26.1.0",
		"postcss": "^8.5.4",
		"postcss-load-config": "^6.0.1",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.4.0",
		"publint": "^0.3.12",
		"svelte": "^5.48.0",
		"svelte-check": "^4.2.1",
		"tailwindcss": "^4.1.8",
		"tslib": "^2.8.1",
		"typescript": "^5.8.3",
		"vite": "^7.1.12",
		"vitest": "^4.0.10"
	},
	"scripts": {
		"dev": "svelte-kit sync && vite dev",
		"build": "svelte-kit sync && vite build && npm run package && rm dist/assets/images/svelvunity-logo.png",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin prettier-plugin-svelte . --check . && eslint .",
		"format": "prettier --plugin prettier-plugin-svelte . --write .",
		"test": "vitest",
		"coverage": "vitest run --coverage"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts"
}
